
{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "strict": 0,
    "new-cap": 0,
    "comma-dangle": [1, "always-multiline"],
    "react/jsx-uses-react": 1,
    "react/sort-comp": 0,
    "react/jsx-filename-extension": 0,
    "flow-vars/define-flow-type": 1,
    "flow-vars/use-flow-type": 1
  },
  "plugins": [
    "react",
    "flow-vars"
  ]
}
